{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug nitcbase",
      "cwd": "${workspaceFolder}",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/NITCbase/mynitcbase/nitcbase-debug",
      "stopAtEntry": false,
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "Build debug nitcbase",
      "miDebuggerPath": "/usr/bin/gdb"
    },
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [],
      "stopAtEntry": false,
      "externalConsole": false,
      "cwd": "/home/ubuntu/Desktop/DBMS/NITCbase/mynitcbase",
      "program": "/home/ubuntu/Desktop/DBMS/NITCbase/mynitcbase/build/Debug/outDebug",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}